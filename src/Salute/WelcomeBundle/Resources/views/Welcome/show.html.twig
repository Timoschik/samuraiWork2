{% extends '::base.html.twig' %}
    {% block stylesheets %}
        <link href="{{ asset('css/style.css') }}" type="text/css" rel="stylesheet" />
    {% endblock %}

{% block body %}
    <ul>
        {% for one in samurai %}
            {% set groupName = one.advancedphp ? one.advancedphp.name : ' NO_GROUP' %}
            <li>{{ one.name ~ '  ' ~ one.skill ~ ' ' ~ groupName }}</li>
        {% endfor %}
    </ul>
 {% endblock %}
